@{
    ViewData["Title"] = "Login";
}<br />
<br />
    <div class="card border-dark mb-3" style="width: 35rem;margin:auto;">
        <div class="card-body text-dark">
            <form>
                <div class="form-group">
                     <label for="Email"><h5><i class="fas fa-users-cog"></i> &nbsp;Correo electronico</h5></label>
                    <input type="email" class="form-control" id="Email" aria-describedby="emailHelp" placeholder="Ingrese correo electronico">
                    
                </div>
                <div class="form-group">
                    <label for="Password"><h5><i class="fas fa-unlock-alt"></i> &nbsp;Contraseña</h5></label>
                    <input type="password" class="form-control" id="Password" placeholder="Ingrese su contraseña">
                </div>
                <button type="submit" class="btn btn-outline-dark">Ingresar</button>
            </form>
        </div>
    </div>
